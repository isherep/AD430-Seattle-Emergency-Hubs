<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cap.emergencyhubsofseattle">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.cap.emergencyhubsofseattle.TheApp"
        android:allowBackup="true"
        android:icon="@drawable/emerfigtwo_512x512"
        android:label="@string/app_name"
        android:roundIcon="@drawable/emerfigtwo_512x512"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.cap.emergencyhubsofseattle.AboutActivity"
            android:label="@string/title_activity_abouts"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName="com.cap.emergencyhubsofseattle.MainActivity" />
        <activity
        android:name="com.cap.emergencyhubsofseattle.ResourseActivity"
        android:label="@string/title_activity_resourse"
        android:parentActivityName="com.cap.emergencyhubsofseattle.MainActivity"
        android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.cap.emergencyhubsofseattle.AnnouncementsActivity"
            android:label="Announcements"
            android:parentActivityName="com.cap.emergencyhubsofseattle.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.cap.emergencyhubsofseattle.AnnouncementDetailsActivity"
            android:label="Announcement Details"
            android:parentActivityName="com.cap.emergencyhubsofseattle.AnnouncementsActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.cap.emergencyhubsofseattle.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cap.emergencyhubsofseattle.SelectedNeighborhoods"
            android:label="@string/title_selected_neighborhoods"
            android:parentActivityName="com.cap.emergencyhubsofseattle.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.cap.emergencyhubsofseattle.FaqMain"
            android:label="@string/FaqMain"
            android:parentActivityName="com.cap.emergencyhubsofseattle.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
<!--            <intent-filter>-->
<!--                <action android:name="android.intent.action.MAIN" />-->
<!--                <category android:name="android.intent.category.LAUNCHER" />-->
<!--            </intent-filter>-->
        </activity>

        <activity
            android:name="com.cap.emergencyhubsofseattle.ContactActivity"
            android:label="CONTACTS"
            android:parentActivityName="com.cap.emergencyhubsofseattle.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />



    </application>

</manifest>